{
  "name": "@mychiara/lights",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack-dev-server --env.client --env.serve --progress",
    "start:aot": "webpack-dev-server --env.client --env.serve --env.aot --progress",
    "build:client": "webpack --env.client --env.aot --progress --hide-modules",
    "build:client:prod": "webpack --env.client --env.aot -p --progress --hide-modules",
    "build:server": "webpack --env.server --progress --hide-modules",
    "build:server:prod": "webpack --env.server --env.aot -p --progress --hide-modules",
    "build": "npm run build:client && npm run build:server",
    "build:prod": "npm run build:client:prod && npm run build:server:prod",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist",
    "server": "node dist/server.js",
    "dev": "webpack --env.server --progress --hide-modules --watch & webpack-dev-server --env.client --env.serve --progress",
    "dev:aot": "webpack --env.aot --env.server --progress --hide-modules --watch & webpack-dev-server --env.aot --env.client --env.serve --progress",
    "watch": "nodemon ./dist/server.js",
    "test": "karma start",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "protractor ./protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mychiara/lights.git"
  },
  "authors": [
    "Andy Ruck"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mychiara/lights#readme",
  "dependencies": {
    "@angular/animations": "^4.2.5",
    "@angular/common": "^4.2.5",
    "@angular/compiler": "^4.2.5",
    "@angular/core": "^4.2.5",
    "@angular/forms": "^4.2.5",
    "@angular/http": "^4.2.5",
    "@angular/platform-browser": "^4.2.5",
    "@angular/platform-browser-dynamic": "^4.2.5",
    "@angular/platform-server": "^4.2.5",
    "@angular/router": "^4.2.5",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "rxjs": "^5.4.1",
    "zone.js": "~0.8.12"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^4.2.5",
    "@ngtools/webpack": "^1.5.0",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.47",
    "codelyzer": "^3.1.1",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.1.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.3",
    "ng-router-loader": "^2.1.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "portfinder": "^1.0.13",
    "protractor": "^5.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "script-ext-html-webpack-plugin": "^1.8.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.5.3",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.0"
  }
}
